    "@ionic-native/twitter-connect": "^5.30.0",
    "@ionic/core": "^5.5.2",
    "@madnesslabs/enjin-editor": "^1.0.4",
    "debounce-decorator": "^1.0.6",
    "firebase": "^8.2.0"
  },
  "devDependencies": {
    "@madnesslabs/fireenjin": "^1.4.0",
    "@stencil/core": "^2.3.0",
