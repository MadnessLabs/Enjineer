{
  "name": "@madnesslabs/enjineer",
  "private": false,
  "version": "1.0.0",
  "description": "The Enjineer app by Madness Labs",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "build:prerender": "stencil build --prerender",
    "firebase:live": "firebase use live",
    "firebase:local": "firebase use local",
    "deploy": "npm run build && firebase deploy",
    "deploy:local": "npm run env:local && npm run deploy",
    "deploy:live": "npm run env:live && npm run deploy",
    "env": "fireenjin env",
    "env:local": "npm run env local && firebase use local",
    "env:live": "npm run env live && firebase use live",
    "generate": "stencil generate",
    "start": "npm run env local && stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.19.0",
    "@editorjs/embed": "^2.4.0",
    "@editorjs/header": "^2.6.1",
    "@editorjs/image": "^2.6.0",
    "@editorjs/list": "^1.6.1",
    "@editorjs/table": "^1.3.0",
    "@ionic-native/core": "^5.30.0",
    "@ionic-native/facebook": "^5.30.0",
    "@ionic-native/geolocation": "^5.30.0",
    "@ionic-native/google-plus": "^5.30.0",
    "@ionic-native/social-sharing": "^5.30.0",
    "@ionic-native/twitter-connect": "^5.30.0",
    "@ionic/core": "^5.5.0",
    "editorjs-drag-drop": "^0.1.1",
    "editorjs-paragraph-with-alignment": "^1.1.0",
    "firebase": "^8.1.1",
    "split-me": "^1.2.2"
  },
  "devDependencies": {
    "@madnesslabs/fireenjin": "^1.4.0",
